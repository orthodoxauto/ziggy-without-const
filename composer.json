{
    "name": "orthodoxauto/ziggy-without-const",
    "description": "Identical to ziggy but uses var instead of const.",
    "keywords": ["laravel", "routes", "javascript", "ziggy"],
    "homepage": "https://github.com/orthodoxauto/ziggy-without-const",
    "license": "MIT",
    "require": {
        "laravel/framework": ">=5.4@dev"
    },
    "authors": [
        {
            "name": "CashforCars.io",
            "email": "admin@cashforcars.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tightenco\\Ziggy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\Ziggy\\ZiggyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
